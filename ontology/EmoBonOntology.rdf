<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.embrc.eu/emobon/EmoBonOntology#"
     xml:base="http://www.embrc.eu/emobon/EmoBonOntology"
     xmlns:mr="http://marineregions.org/ns/ontology#"
     xmlns:sf="http://www.opengis.net/ont/sf#"
     xmlns:sh="http://www.w3.org/ns/shacl#"
     xmlns:dwc="http://rs.tdwg.org/dwc/terms/"
     xmlns:geo="http://www.opengis.net/ont/geosparql#"
     xmlns:obo="http://purl.obolibrary.org/obo/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:schema="http://schema.org/"
     xmlns:emobon="http://www.embrc.eu/emobon/EmoBonOntology#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:sosa="http://www.w3.org/ns/sosa/"
     xmlns:qudt="http://qudt.org/schema/qudt/">
    <owl:Ontology rdf:about="http://www.embrc.eu/emobon/EmoBonOntology">
        <owl:versionIRI rdf:resource="http://www.embrc.eu/emobon/v.0.0.1/EmoBonOntology.rdf"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#wktLiteral -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#wktLiteral"/>
    


    <!-- http://www.w3.org/ns/shacl#IRI -->

    <rdfs:Datatype rdf:about="http://www.w3.org/ns/shacl#IRI"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://schema.org/address -->

    <owl:ObjectProperty rdf:about="http://schema.org/address">
        <rdfs:range rdf:resource="http://schema.org/PostalAddress"/>
    </owl:ObjectProperty>
    


    <!-- http://www.embrc.eu/emobon/EmoBonOntology#broadBiome -->

    <owl:ObjectProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#broadBiome">
        <rdfs:subPropertyOf rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#hasBiome"/>
    </owl:ObjectProperty>
    


    <!-- http://www.embrc.eu/emobon/EmoBonOntology#geoLocation -->

    <owl:ObjectProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#geoLocation">
        <rdfs:range rdf:resource="http://www.opengis.net/ont/sf#Point"/>
    </owl:ObjectProperty>
    


    <!-- http://www.embrc.eu/emobon/EmoBonOntology#hasBiome -->

    <owl:ObjectProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#hasBiome">
        <rdfs:domain rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/ENVO_00000428"/>
    </owl:ObjectProperty>
    


    <!-- http://www.embrc.eu/emobon/EmoBonOntology#localBiome -->

    <owl:ObjectProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#localBiome">
        <rdfs:subPropertyOf rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#hasBiome"/>
    </owl:ObjectProperty>
    


    <!-- http://www.embrc.eu/emobon/EmoBonOntology#sampleType -->

    <owl:ObjectProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#sampleType">
        <rdfs:range rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#SampleType"/>
    </owl:ObjectProperty>
    

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#linkedToObservatory -->

    <owl:ObjectProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#linkedToObservatory">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">The observatory that is linked to the sampling event.</rdfs:comment>
        <rdfs:label xml:lang="en">is linked to observatory</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sampling"/>
        <rdfs:range rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
    </owl:ObjectProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://schema.org/addressCountry -->

    <owl:DatatypeProperty rdf:about="http://schema.org/addressCountry">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/description -->

    <owl:DatatypeProperty rdf:about="http://schema.org/description">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/identifier -->

    <owl:DatatypeProperty rdf:about="http://schema.org/identifier">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/legalName -->

    <owl:DatatypeProperty rdf:about="http://schema.org/legalName">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://schema.org/Organization"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/postalCode -->

    <owl:DatatypeProperty rdf:about="http://schema.org/postalCode"/>
    


    <!-- http://schema.org/streetAddress -->

    <owl:DatatypeProperty rdf:about="http://schema.org/streetAddress">
        <rdfs:domain rdf:resource="http://schema.org/PostalAddress"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.embrc.eu/emo-bon/EmoBonOntology#edmoId -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emo-bon/EmoBonOntology#edmoId">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.embrc.eu/emo-bon/EmoBonOntology#Organization"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.embrc.eu/emobon/EmoBonOntology#marineRegion -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#marineRegion">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.embrc.eu/emobon/EmoBonOntology#observatoryId -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#observatoryId">
        <rdfs:domain rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.embrc.eu/emobon/EmoBonOntology#observatorySampleId -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#observatorySampleId">
        <rdfs:domain rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#asWKT -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#asWKT">
        <rdfs:domain rdf:resource="http://www.opengis.net/ont/sf#Geometry"/>
        <rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#wktLiteral"/>
    </owl:DatatypeProperty>
    

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#enaProjAccNum -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#enaProjAccNum">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">ENA project accession number associated to this observatory.</rdfs:comment>
        <rdfs:label xml:lang="en">has ENA project accession number</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Project"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#enaStudyAccNum -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#enaStudyAccNum">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">ENA study accession number associated to the observatory.</rdfs:comment>
        <rdfs:label xml:lang="en">has ENA study accession number</rdfs:label>
        <rdfs:domain rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#enaSampleAccNum -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#enaSampleAccNum">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">ENA sample accession number associated with the sample.</rdfs:comment>
        <rdfs:label xml:lang="en">has ENA sample accession number</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#hasMixsPackage -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#hasMixsPackage">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">The MIxS extension for reporting of measurements and observations obtained from one or more of the environments where a sample was obtained.</rdfs:comment>
        <rdfs:label xml:lang="en">has Mixs Package</rdfs:label>
        <rdfs:domain rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#marineRegionName -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#marineRegionName">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">The name of the marine region</rdfs:comment>
        <rdfs:label xml:lang="en">Name associated with the marine region</rdfs:label>
        <rdfs:domain rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#waterColumnDepth -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#waterColumnDepth">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Measument of the total depth of the water column in meters</rdfs:comment>
        <rdfs:label xml:lang="en">Total depth of water column</rdfs:label>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="http://www.w3.org/ns/sosa/Sampling"/>
                <rdf:Description rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#originCountry -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#originCountry">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">The geographical origin of the sample as defined by the country name.</rdfs:comment>
        <rdfs:label xml:lang="en">Country of origin of the sample</rdfs:label>
        <rdfs:domain>
        <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.w3.org/ns/sosa/Sampling"/>
            <rdf:Description rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
            </owl:unionOf>
        </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#soilType -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#soilType">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Information about the sediment type of the sample based on major constituents</rdfs:comment>
        <rdfs:label xml:lang="en">Sediment type of a soil sample</rdfs:label>
        <rdfs:domain rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology#Observatory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#samplingDepth -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#samplingDepth">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Measurement of the sampling depth of the water column in meters</rdfs:comment>
        <rdfs:label xml:lang="en">Sampling depth of water column</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sampling"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#sampleDepth -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#sampleDepth">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Measurement of the sample depth of the water column in meters</rdfs:comment>
        <rdfs:label xml:lang="en">Sampling depth of water column</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#tidalStage -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#tidalStage">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Tidal stage at the moment of the sampling event, expected values are: low_tide, ebb_tide, flood_tide, high_tide</rdfs:comment>
        <rdfs:label xml:lang="en">Tidal stage</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sampling"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#storageTemp -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#storageTemp">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Temperature at which the sample was stored during a storage activity.</rdfs:comment>
        <rdfs:label xml:lang="en">Storage temperature</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#storageLoc -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#storageLoc">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Location at which the sample was stored during a storage activity.</rdfs:comment>
        <rdfs:label xml:lang="en">Storage location</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#longStorage -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#longStorage">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Indicates if the sample is stored long term.</rdfs:comment>
        <rdfs:label xml:lang="en">Long storage</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#membrCut -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#membrCut">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Indicates if the membrane was cut in half after filtration</rdfs:comment>
        <rdfs:label xml:lang="en">Membrane cut</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#filtrationTime -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#filtrationTime">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Indicates how long from sampling until start of filtration, 'fi' indicates filtration was done in the field.</rdfs:comment>
        <rdfs:label xml:lang="en">Filtration time</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#hasSampleSize -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#hasSampleSize">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">The size of the collected sample</rdfs:comment>
        <rdfs:label xml:lang="en">has ENA sample accession number</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#failure -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#failure">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Indication of failure in data generation, which may have occurred at any stage of the workflow; takes True (T) or False (F) values.</rdfs:comment>
        <rdfs:label xml:lang="en">sample failure</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#failureComment -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#failureComment">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Description of failure cause</rdfs:comment>
        <rdfs:label xml:lang="en">sample failure comment</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#sampleReplicate -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#sampleReplicate">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">Replicate number of the sample or Blank</rdfs:comment>
        <rdfs:label xml:lang="en">sample replicate</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#filterSizeRange -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#filterSizeRange">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
        <rdfs:comment xml:lang="en">The Range of sieve pore size used to pre-filter/pre-sort and retain the macrobenthos sample.</rdfs:comment>
        <rdfs:label xml:lang="en">filter size range</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Sample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- http://www.embrc.eu/emobon/EmoBonOntology#lowerFilterSize -->

    <owl:DatatypeProperty rdf:about="http://www.embrc.eu/emobon/EmoBonOntology#lowerFilterSize">
        <rdfs:isDefinedBy rdf:resource="http://www.embrc.eu/emobon/EmoBonOntology"/>
